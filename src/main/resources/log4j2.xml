<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <File name="LogFile" fileName="logs/jdisco.log">
      <PatternLayout>
        <Pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Logger name="org.phbs.discogs" level="debug" >
      <AppenderRef ref="LogFile"/>
    </Logger>
    <Root level="error">
      <AppenderRef ref="LogFile"/>
    </Root>
  </Loggers>
</Configuration>